# Server
PORT=3000
NODE_ENV=development

# Supabase (Service Role key is required by backend)
SUPABASE_URL=
SUPABASE_SERVICE_KEY=

# RAG integration (single-port via Python child process)
# Optional: absolute path to Python executable if not on PATH
# e.g., C:\\Users\\you\\AppData\\Local\\Programs\\Python\\Python312\\python.exe
RAG_PYTHON_PATH=

# Optional: absolute path to rag_runner.py; if empty, backend resolves relative to repo
# e.g., C:\\Code\\DACN_MindMapNote\\Embedding_langchain\\scripts\\rag_runner.py
RAG_RUNNER_PATH=

# Optional: timeout in ms for each RAG call (default 60000)
RAG_TIMEOUT_MS=60000
